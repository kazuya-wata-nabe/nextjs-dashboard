{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended",
    "plugin:unicorn/recommended",
    "plugin:jsx-a11y/strict",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "unicorn/filename-case": [
      "error",
      {
        "case": "kebabCase",
        "ignore": [
          ""
        ]
      }
    ],
    "unicorn/prevent-abbreviations": [
      "error",
      {
        "allowList": {
          "Props": true,
          "props": true
        }
      }
    ]
  }
}